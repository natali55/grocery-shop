{
  "name": "infra",
  "version": "0.1.0",
  "bin": {
    "infra": "bin/infra.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "npm run build && cdk synth && cdk deploy --all",
    "invalidate": "aws cloudfront create-invalidation --distribution-id E1798PE220MWUQ --paths '/resources/*'",
    "destroy": "cdk destroy",
    "papulate-table": "node lib/shared/populate-table.js",
    "scan-products-table": "aws dynamodb scan --table-name Products",
    "scan-stock-table": "aws dynamodb scan --table-name Stock"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.137",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "aws-cdk": "2.135.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.3"
  },
  "dependencies": {
    "@aws-cdk/aws-ec2": "^1.204.0",
    "@aws-cdk/aws-rds": "^1.204.0",
    "@aws-sdk/client-dynamodb": "^3.556.0",
    "@aws-sdk/client-s3": "^3.569.0",
    "@aws-sdk/client-sns": "^3.574.0",
    "@aws-sdk/client-sqs": "^3.572.0",
    "@aws-sdk/s3-request-presigner": "^3.569.0",
    "@types/uuid": "^9.0.8",
    "aws-cdk-lib": "2.135.0",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1613.0",
    "constructs": "^10.0.0",
    "csv-parser": "^3.0.0",
    "source-map-support": "^0.5.21",
    "uuid": "^9.0.1"
  }
}
